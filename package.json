{
  "name": "unusual-words-dashboard",
  "version": "0.0.1",
  "description": "Dashboard for unusual words and their synonyms",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node src/server.js",
    "clean": "rimraf dest",
    "prebuild": "npm run clean && mkdir -p dest",
    "build": "webpack",
    "dev": "npm run dev:web & npm run dev:static",
    "dev:web": "node src/server.js",
    "dev:static": "webpack-dev-server",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js \"test/**/*@(.js)\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Mike Cravey <mike@craveytrain.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.4.1",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "mocha-junit-reporter": "^1.12.0",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.3",
    "rimraf": "^2.5.4",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  },
  "dependencies": {
    "good": "^7.0.2",
    "good-console": "^6.1.2",
    "good-squeeze": "^4.0.0",
    "h2o2": "^5.1.1",
    "hapi": "^15.0.3",
    "hapi-webpack-plugin": "^1.3.0",
    "inert": "^4.0.2",
    "react": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "whatwg-fetch": "^1.0.0"
  }
}
